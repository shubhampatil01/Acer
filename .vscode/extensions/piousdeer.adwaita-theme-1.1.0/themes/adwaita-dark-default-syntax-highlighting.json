      "settings": {
        "foreground": "#569CD6"
      }
    },
    {
      "scope": [
        "string",
        "meta.embedded.assembly"
      ],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": "string.tag",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
